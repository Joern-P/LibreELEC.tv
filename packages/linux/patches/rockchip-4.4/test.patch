From bd5867cc50b66a0bccdb7a76d55f6ba95972ec67 Mon Sep 17 00:00:00 2001
From: =?UTF-8?q?j=C3=B6rn?= <ahap1@web.de>
Date: Thu, 3 Sep 2020 13:42:17 +0200
Subject: [PATCH] add 96 Mhz Cpu freq

---
 arch/arm64/boot/dts/rockchip/rk3399-opp.dtsi | 22 ++++++++++++++++++--
 1 file changed, 20 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/rockchip/rk3399-opp.dtsi b/arch/arm64/boot/dts/rockchip/rk3399-opp.dtsi
index f1178767bdbe..0bf8883696fe 100644
--- a/arch/arm64/boot/dts/rockchip/rk3399-opp.dtsi
+++ b/arch/arm64/boot/dts/rockchip/rk3399-opp.dtsi
@@ -78,6 +78,15 @@
 		rockchip,pvtm-temp-prop = <115 66>;
 		rockchip,thermal-zone = "soc-thermal";
 		
+		opp-96000000 {
+			opp-hz = /bits/ 64 <96000000>;
+			opp-microvolt = <775000 775000 1250000>;
+			opp-microvolt-L0 = <775000 775000 1250000>;
+			opp-microvolt-L1 = <775000 775000 1250000>;
+			opp-microvolt-L2 = <775000 775000 1250000>;
+			opp-microvolt-L3 = <775000 775000 1250000>;
+			clock-latency-ns = <40000>;
+		};
 		opp-216000000 {
 			opp-hz = /bits/ 64 <216000000>;
 			opp-microvolt = <800000 800000 1250000>;
